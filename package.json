{
  "name": "seeded-rand",
  "version": "3.0.0",
  "description": "A simple seeded pseudo-random number generator",
  "main": "dist/jsrand.js",
  "module": "dist/jsrand.esm.js",
  "types": "dist/jsrand.d.ts",
  "exports": {
    ".": {
      "import": "./dist/jsrand.esm.js",
      "require": "./dist/jsrand.js",
      "types": "./dist/jsrand.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:module && npm run build:esm && npm run build:browser",
    "build:module": "BABEL_ENV=dev babel src/jsrand.js --out-file dist/jsrand.js",
    "build:esm": "BABEL_ENV=esm babel src/jsrand.js --out-file dist/jsrand.esm.js",
    "build:browser": "BABEL_ENV=production browserify --debug src/jsrand_browser.js -t [babelify] | exorcist dist/jsrand.min.js.map > dist/jsrand.min.js",
    "flow": "flow",
    "test": "qunit test/tests.js",
    "test:coverage": "c8 --reporter=lcov --reporter=text npm test",
    "test:browser": "opener test/browser_tests.html",
    "benchmark": "node benchmark/performance.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DomenicoDeFelice/jsrand.git"
  },
  "keywords": [
    "prng",
    "seed",
    "seeded",
    "pseudorandom",
    "pseudo-random",
    "random",
    "generator",
    "marsaglia",
    "deterministic",
    "reproducible",
    "testing",
    "typescript"
  ],
  "author": "Dom De Felice",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DomenicoDeFelice/jsrand/issues"
  },
  "homepage": "https://github.com/DomenicoDeFelice/jsrand#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-flow": "^7.10.1",
    "array-equal": "^1.0.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-minify": "^0.5.1",
    "babelify": "^10.0.0",
    "c8": "^10.1.3",
    "exorcist": "^2.0.0",
    "flow-bin": "^0.127.0",
    "opener": "^1.5.1",
    "prettier": "^2.0.5",
    "qunit": "^2.10.0"
  },
  "browserslist": "> 0.25%, not dead"
}
